starFig <- function(nD){
n <- nD
ln <- 0:n
m <- (n-1)/2
y <- cos(2*pi*ln/n)
x <- sin(2*pi*ln/n)
z <- (ln*m)%%n+1
purple <- rgb(.5,0,1)
red <- rgb(1,0,1)
par(fin=c(5,5))
plot(x[z], y[z], type="l", col=purple, lwd=3, xaxt="n", yaxt="n", ylab="", xlab="")
points(x,y, pch=19, col=red)
}
